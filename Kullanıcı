CREATE USER SUBE1_DB_USER IDENTIFIED BY user1
DEFAULT TABLESPACE GALERI_DATA_TS
QUOTA UNLIMITED ON GALERI_DATA_TS
QUOTA UNLIMITED ON GALERI_INDEX_TS
QUOTA UNLIMITED ON SUBE1_ENVANTER_TS;

CREATE USER SUBE2_DB_USER IDENTIFIED BY user2
DEFAULT TABLESPACE GALERI_DATA_TS
QUOTA UNLIMITED ON GALERI_DATA_TS
QUOTA UNLIMITED ON GALERI_INDEX_TS
QUOTA UNLIMITED ON SUBE2_ENVANTER_TS;

CREATE USER SUBE3_DB_USER IDENTIFIED BY user3
DEFAULT TABLESPACE GALERI_DATA_TS
QUOTA UNLIMITED ON GALERI_DATA_TS
QUOTA UNLIMITED ON GALERI_INDEX_TS
QUOTA UNLIMITED ON SUBE3_ENVANTER_TS;

GRANT CONNECT, RESOURCE, CREATE SESSION TO SUBE1_DB_USER, SUBE2_DB_USER, SUBE3_DB_USER;
GRANT CREATE VIEW, CREATE MATERIALIZED VIEW, CREATE SYNONYM TO SUBE1_DB_USER, SUBE2_DB_USER, SUBE3_DB_USER;
